    "pnpm-lock.yaml"
  ],
  "ignoreRegExpList": [
    // Ignore hex colors
    "/#[0-9a-fA-F]{3,8}/g",
    // Ignore base64 strings
    "/[A-Za-z0-9+/]{40,}={0,2}/g",
    // Ignore UUIDs
    "/[0-9a-fA-F]{8}-[0-9a-fA-F]{4}-[0-9a-fA-F]{4}-[0-9a-fA-F]{4}-[0-9a-fA-F]{12}/g",
    // Ignore API keys and tokens
    "/sk-[A-Za-z0-9]{40,}/g",
    // Ignore long hashes
    "/[a-f0-9]{40,}/g"
  ],
  "allowCompoundWords": true,
