{
  "name": "@eh-aye/stts",
  "version": "1.0.0",
  "description": "Smart Text-to-Speech (TTS) integration for Claude Code and other development tools. Hear notifications, tool executions, and more with multiple voice providers.",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "type": "module",
  "bin": {
    "stts": "dist/cli/index.js"
  },
  "scripts": {
    "build": "tsc",
    "dev": "node dev.js",
    "dev:build": "tsc --watch",
    "prepublishOnly": "npm run build && node scripts/prepublish.js",
    "validate": "node scripts/prepublish.js",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "test:ui": "vitest --ui",
    "lint": "eslint src --ext .ts",
    "lint:fix": "eslint src --ext .ts --fix",
    "format": "prettier --write \"src/**/*.{ts,js,json,md}\"",
    "format:check": "prettier --check \"src/**/*.{ts,js,json,md}\"",
    "prepare": "husky",
    "clean": "rm -rf dist",
    "prepack": "npm run clean && npm run build"
  },
  "keywords": [
    "tts",
    "text-to-speech",
    "claude-code",
    "developer-tools",
    "hooks",
    "voice",
    "notifications",
    "elevenlabs",
    "openai",
    "accessibility"
  ],
  "author": {
    "name": "Val Neekman",
    "email": "val@neekware.com"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ehaye/stts.git"
  },
  "bugs": {
    "url": "https://github.com/ehaye/stts/issues"
  },
  "homepage": "https://github.com/ehaye/stts#readme",
  "engines": {
    "node": ">=18.0.0"
  },
  "dependencies": {
    "axios": "^1.6.0",
    "chalk": "^4.1.2",
    "commander": "^11.0.0",
    "dotenv": "^16.0.0",
    "glob": "^10.3.10",
    "prettier": "^3.6.2",
    "say": "^0.16.0",
    "which": "^4.0.0",
    "winston": "^3.11.0"
  },
  "devDependencies": {
    "@semantic-release/changelog": "^6.0.3",
    "@semantic-release/git": "^10.0.1",
    "@types/node": "^20.0.0",
    "@types/which": "^3.0.0",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "@vitest/ui": "^3.2.4",
    "eslint": "^8.0.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.2",
    "husky": "^9.0.0",
    "lint-staged": "^15.2.0",
    "semantic-release": "^23.0.0",
    "tsx": "^4.6.2",
    "typescript": "^5.0.0",
    "vitest": "^3.2.4"
  },
  "files": [
    "dist/**/*",
    "README.md",
    "LICENSE"
  ]
}
